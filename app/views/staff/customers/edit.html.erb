<% @title = "顧客アカウントの編集" %>
<h1><%= @title %></h1>

<div id="genetic-form">
  <%= form_with model: @customer_form, scope: "form", url: staff_customer_path(@customer_form.customer) do |f| %>
    <%= render "form", f: f %>
    <div class="buttons">
      <%= f.submit "更新" %>
      <%= link_to "キャンセル", staff_customers_path %>
    </div>
  <% end %>
</div>
